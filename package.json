{
  "name": "rexpath",
  "version": "1.0.17",
  "description": "A query language for DOM tree. You can use regular expression with xpath (or css-selector) when querying DOM.",
  "main": "dist/index.js",
  "unpkg": "dist/index-web.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fgatherlet/rexpath.git"
  },
  "scripts": {
    "test": "tape t/*.js"
    ,"build": "webpack --config webpack.config.production.js"
    ,"build-development": "webpack --config webpack.config.development.js"
    ,"all": "npm run build; npm run build-development; npm run test"
  },
  "keywords": [
    "xpath"
    ,"regexp"
    ,"regular-expression"
    ,"css"
    ,"querySelector"
    ,"dom"
    ,"regex"
    ,"selector"
  ],
  "author": "fgatherlet",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@webpack-cli/init": "^0.2.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.2.1",
    "tape": "^4.13.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^5.0.0"
  }
}
